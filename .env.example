# Supabase Configuration (reuse from main .env or set here)
SUPABASE_URL=https://sboglbzwiaqcovmlxsga.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# OpenAI Configuration  
OPENAI_API_KEY=your_openai_key_here
OPENAI_MODEL=gpt-4-turbo-preview

# Agent Cost Control Settings
USE_AGENT_BY_DEFAULT=true
MAX_CANDIDATES_FOR_AGENT=50
OPENAI_TIMEOUT=10

# Caching & Rate Limiting
ENABLE_RESULT_CACHING=true
CACHE_TTL_MINUTES=5
RATE_LIMIT_PER_MINUTE=10

# Fallback Strategy
FALLBACK_TO_DIRECT_ON_ERROR=true

# Server Configuration
PORT=8001
CORS_ORIGINS=["http://localhost:5173","https://*.vercel.app"]
